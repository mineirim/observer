		</parameter>
		<queryString>
			<![CDATA[SELECT
     p."nome",
     p."nome",
     p."apresentacao" AS p_apresentacao,
     p."data_inicio" AS p_data_inicio,
     p."data_fim" AS p_data_fim,
     usuarios."nome" AS usuarios_nome
FROM
     "projetos" p INNER JOIN "public"."usuarios" usuarios ON p."coordenador_usuario_id" = usuarios."id"
WHERE
     p.id = $P{projeto_id}]]>
		</queryString>
